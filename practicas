        "sales_by_country = data.groupby('COUNTRY')['AMOUNT'].sum().reset_index() #Sumamos las ventas por país (Utilizamos reset index para convertir el indice a columna)\n",
        "fig_country = px.bar(sales_by_country, x='COUNTRY', y='AMOUNT', title='Distribución de Ventas por País')\n",
        "fig_country.show()"
