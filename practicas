      "cell_type": "markdown",
      "source": [
        "# Pregunta 2: Tendencia y estacionalidad"
      ],
      "metadata": {
        "id": "jW-NMnuCYkOU"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "# Convertir columna de fecha para trabajar con fechas completas\n",
        "data['DATE'] = pd.to_datetime(data[['YEAR', 'MONTH']].assign(DAY=1))\n",
        "\n",
        "# Encontrar el país con menos ventas\n",
        "sales_by_country = data.groupby('COUNTRY')['AMOUNT'].sum().reset_index()\n",
        "country_least_sales = sales_by_country[sales_by_country['AMOUNT'] == sales_by_country['AMOUNT'].min()]['COUNTRY'].values[0]\n",
        "\n",
        "# Filtrar datos para el país con menos ventas\n",
        "data_least_country = data[data['COUNTRY'] == country_least_sales]\n",
        "sales_least_country_date = data_least_country.groupby('DATE')['AMOUNT'].sum().reset_index()\n",
        "\n",
        "# Calcular la tendencia usando un promedio móvil de 12 meses\n",
        "sales_least_country_date['Trend'] = sales_least_country_date['AMOUNT'].rolling(window=12, center=True).mean()\n",
        "\n",
        "# Calcular la estacionalidad como la diferencia entre el valor original y la tendencia\n",
        "sales_least_country_date['Seasonal'] = sales_least_country_date['AMOUNT'] - sales_least_country_date['Trend']\n",
        "\n",
        "# Usar plotly.express para crear un gráfico de líneas\n",
        "fig = px.line(sales_least_country_date, x='DATE', y=['AMOUNT', 'Trend', 'Seasonal'],\n",
        "              labels={'value': 'Monto de Ventas', 'variable': 'Serie'},\n",
        "              title=f'Tendencia y Estacionalidad en {country_least_sales}')\n",
        "\n",
        "# Mostrar el gráfico\n",
        "fig.show()\n"
      ],
